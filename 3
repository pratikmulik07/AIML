game_map={
    "Camp":["Forest","River"],
    
    "Forest":["Tresure Room"],
    
    "River":["Tresure Room"],

    "Tresure Room":[]
}

start_location = "Camp"
target_location = "Tresure Room"

visited=set()

def dfs_traverse(node):
    print("Visting:",node)
    visited.add(node)
    for next_node in game_map[node]:
        if next_node not in visited:
            dfs_traverse(next_node)

print("Order of Traversal(DFS)\n")
dfs_traverse(start_location)

def dfs_paths(current, goal, path=[]):
    path = path + [current]

    if current == goal:
        return [path]

    paths = []
    for next_node in game_map[current]:
        if next_node not in path:
            new_paths = dfs_paths(next_node, goal, path)
            for p in new_paths:
                paths.append(p)

    return paths

print("\nAll possible paths to traverse:\n")
paths = dfs_paths(start_location, target_location)

for p in paths:
    print(" -> ".join(p))
